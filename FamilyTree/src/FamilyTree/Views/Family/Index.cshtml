@model List<FamilyTree.Models.FamilyMember>

@if(Model.Count != 0)
{
    <table>
        <tr>
            <th>
                Name
            </th>
            <th>
                Relation
            </th>
        </tr>
    </table>
    foreach(var f in Model)
    {
        <tr>
            <td>
                @f.Name
            </td>
            <td>
                @f.Relation
            </td>
        </tr>

    }
}
else
{
    <p> No family members added for this users</p>
}